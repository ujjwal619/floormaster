{
  "private": true,
  "scripts": {
    "admin-dev": "yarn admin-development",
    "admin-development": "cross-env process.env.section=admin NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "admin-watch": "yarn admin-development --watch",
    "admin-watch-poll": "yarn admin-development --watch-poll",
    "admin-prod": "yarn admin-production",
    "admin-production": "cross-env process.env.section=admin NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "auth-dev": "yarn auth-development",
    "auth-development": "cross-env process.env.section=auth NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "auth-watch": "yarn auth-development --watch",
    "auth-prod": "yarn auth-production",
    "auth-production": "cross-env process.env.section=auth NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "dev": "yarn development",
    "development": "yarn admin-dev | yarn auth-dev",
    "watch": "yarn admin-watch | yarn auth-watch",
    "watch-poll": "yarn watch --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "yarn production",
    "production": "yarn admin-prod | yarn auth-prod"
  },
  "devDependencies": {
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "cross-env": "7.0.3",
    "laravel-mix": "^2.0",
    "popper.js": "^1.12",
    "vee-validate": "^2.2.13"
  },
  "dependencies": {
    "@deveodk/vue-toastr": "^1.0.4",
    "accounting": "^0.4.1",
    "animate.css": "^3.6.1",
    "axios": "^0.18",
    "bootstrap": "^4.1.2",
    "collect.js": "^4.0.28",
    "date-fns": "^2.9.0",
    "flatpickr": "^4.6.3",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2",
    "jquery-smooth-scroll": "^2.2.0",
    "js-cookie": "^2.2.0",
    "malihu-custom-scrollbar-plugin": "^3.1.5",
    "moment": "^2.24.0",
    "shortid": "^2.2.15",
    "socicon": "^3.0.5",
    "sweetalert2": "^7.25.6",
    "v-click-outside": "^3.0.0",
    "vue": "^2.6.12",
    "vue-multiselect": "^2.1.3",
    "wnumb": "^1.1.0"
  }
}
